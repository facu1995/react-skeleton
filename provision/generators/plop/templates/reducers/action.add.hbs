export enum typeAction{
    'increaseBy'='increaseBy',
    'reset'='reset'
}

export type {{properCase reducerName}}Action =
    | { type: typeAction.increaseBy,payload: { value: number } }
    | { type: typeAction.reset };

export const doReset = (): {{properCase reducerName}}Action => ({
    type:  typeAction.reset
})

export const doIncrement = (value:number): {{properCase reducerName}}Action => ({ type: typeAction.increaseBy, payload: { value } })
